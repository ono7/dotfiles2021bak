{
  "diagnostic.displayByAle": true,
  "diagnostic.messageDelay": 200,
  "python.jediEnabled": true,
  "python.venvFolders": ["venv", ".virtualenvs"],
  "python.pythonPath": "~/.virtualenvs/prod3/bin/python",
  "python.analysis.diagnosticEnabled": false,
  "python.linting.pylintPath": "~/.virtualenvs/prod3/bin/pylint",
  "python.linting.pylintEnabled": true,
  // TODO: move format jobs to format.vim
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "yaml",
    "typescript",
    "css"
  ],
  "python.linting.enabled": true,
  "python.linting.maxNumberOfProblems": 100,
  "python.linting.ignorePatterns": [".vscode/*.py", "**/site-packages/**/*.py"],
  "javascript.suggestionActions.enabled": false,
  "tsserver.npm": "~/.npm-packages/bin/npm",
  "javascript.preferences.quoteStyle": "single",
  "prettier.singleQuote": true,
  "Lua.runtime.path": ["?.lua"],
  "Lua.runtime.version": "Lua 5.1",
  "Lua.workspace.maxPreload": 1000,
  "Lua.workspace.preloadFileSize": 200,
  "Lua.workspace.ignoreDir": [".vscode", ".dotfiles", "~/.dotfiles"],
  "Lua.diagnostics.disable": [
    "lowercase-global",
    "unused-local",
    "unused-vararg"
  ],
  "Lua.diagnostics.enable": true,
  "Lua.diagnostics.globals": [
    "hs",
    "vim",
    "it",
    "describe",
    "before_each",
    "after_each"
  ],
  "Lua.diagnostics.neededFileStatus": "Opened",
  "Lua.completion.keywordSnippet": "Disable",
  "Lua.telemetry.enable": null
}
